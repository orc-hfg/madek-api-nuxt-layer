{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": [
		"app/stores/**/*.ts",
		"server/api/*.ts",
		"server/utils/*.ts"
	],
	"project": [
		"app/**",
		"server/**",
		"shared/**"
	],
	"ignore": ["app/plugins/**", "app/composables/**", "app/utils/**", "shared/constants/**", "server/constants/**"],
	"ignoreDependencies": [
		"@nuxt/devtools",
		"@nuxt/eslint",
		"@pinia/nuxt",
		"vue-tsc"
	]
}
