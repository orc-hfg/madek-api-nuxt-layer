{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": [
		"app/composables/*.ts",
		"app/plugins/*.ts",
		"app/repositories/*.ts",
		"app/services/*.ts",
		"app/stores/*.ts",
		"app/utils/*.ts",
		"server/api/*.ts",
		"server/constants/*.ts",
		"server/utils/*.ts",
		"shared/constants/*.ts",
		"shared/types/*.ts",
		"shared/utils/*.ts",
		"tests/mocks/logger.ts",
		"**/*.setup.ts",
		"**/*.test.ts"
	],
	"project": [
		"app/**",
		"server/**",
		"shared/**",
		"tests/**"
	],
	"ignoreDependencies": [
		"@nuxt/devtools",
		"@nuxt/kit",
		"h3",
		"nitropack",
		"ofetch",
		"vue"
	]
}
