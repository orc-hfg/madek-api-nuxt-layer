{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": [
		"app/stores/**/*.ts",
		"server/api/*.ts",
		"server/constants/*.ts",
		"server/utils/*.ts",
		"shared/constants/*.ts",
		"shared/utils/*.ts",
		"tests/**/*.ts"
	],
	"project": [
		"app/**",
		"server/**",
		"shared/**",
		"tests/**"
	],
	"ignoreDependencies": [
		"@nuxt/devtools",
		"@nuxt/eslint",
		"@pinia/nuxt",
		"pinia",
		"vue"
	]
}
